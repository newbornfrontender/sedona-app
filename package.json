{
  "name": "sedona-app",
  "version": "1.0.0",
  "description": "Верстка адаптивного шаблона \"Sedona\" при помощи Vue CLI с хостингом на Firebase",
  "private": true,
  "scripts": {
    "prettier:check": "tslint-config-prettier-check ./tslint.json",
    "prettier:write": "prettier ./{src,public,test}/**/*.{js,jsx,ts,tsx,vue} --write",
    "prettier:write:all": "prettier ./**/* --write",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "serve": "vue-cli-service serve",
    "prebuild": "yarn prettier:check && yarn prettier:write",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "7.0.0-bridge.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.1.3",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.1.0-dev.20180818",
    "vue-template-compiler": "^2.5.17"
  }
}
